{
    "name": "__MSG_dev_efaz_bundled_extension_exName__",
    "description": "__MSG_dev_efaz_bundled_extension_exDescription__",
    "version": "1.7.1",
    "manifest_version": 3,
    "author": "EfazDev",
    "homepage_url": "https://www.efaz.dev/roblox-extension",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "default_locale": "en",
    "background": {
        "service_worker": "main.js"
    },
    "externally_connectable": {
        "matches": [
            "https://cdn.efaz.dev/*"
        ]
    },
    "content_scripts": [
        {
            "all_frames": true,
            "js": [
                "dev.efaz.communities_renamer/community_rename.js",
                "dev.efaz.roblox_logo_changer/logo_changer.js",
                "dev.efaz.marketplace_renamer/marketplace_rename.js",
                "dev.efaz.charts_renamer/charts_rename.js",
                "dev.efaz.foundation_color_accents/foundation_colors.js",
                "dev.efaz.experiences_renamer/experiences_rename.js",
                "dev.efaz.connections_renamer/connections_rename.js",
                "dev.efaz.remove_builder_font/builder_font_remover.js",
                "dev.efaz.verified_badge_add_on/fake_verified_loader.js"
            ],
            "css": [],
            "matches": [
                "*://*.roblox.com/*"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        {
            "matches": [
                "*://*.roblox.com/*"
            ],
            "resources": [
                "dev.efaz.communities_renamer/settings.json",
                "dev.efaz.roblox_logo_changer/settings.json",
                "dev.efaz.marketplace_renamer/settings.json",
                "dev.efaz.charts_renamer/settings.json",
                "dev.efaz.foundation_color_accents/settings.json",
                "dev.efaz.experiences_renamer/settings.json",
                "dev.efaz.connections_renamer/settings.json",
                "dev.efaz.remove_builder_font/change_font.css",
                "dev.efaz.remove_builder_font/change_font2.css",
                "dev.efaz.remove_builder_font/creator_dashboard.css",
                "dev.efaz.remove_builder_font/devforum_font.css",
                "dev.efaz.remove_builder_font/global_font.css",
                "dev.efaz.remove_builder_font/settings.json",
                "dev.efaz.verified_badge_add_on/settings.json"
            ],
            "use_dynamic_url": true
        }
    ],
    "action": {
        "default_popup": "settings.html",
        "default_title": "__MSG_dev_efaz_bundled_extension_exName__",
        "default_icon": "icon128.png"
    },
    "content_security_policy": {
        "sandbox": "sandbox allow-scripts; script-src * 'wasm-unsafe-eval';"
    },
    "sandbox": {
        "pages": []
    },
    "permissions": [
        "storage",
        "unlimitedStorage"
    ],
    "host_permissions": [
        "https://*.roblox.com/*"
    ]
}